{
  "pipeline_definition": [
    {
      "module": "aiko_services.elements.video_io",
      "name": "VideoReadFile",
      "parameters": {
        "video_pathname": "astra_brief.mp4"
      },
      "successors": {
        "alternate": [
          "ImageAnnotate2"
        ],
        "default": [
          "ImageAnnotate1"
        ]
      }
    },
    {
      "module": "aiko_services.elements.image_io",
      "name": "ImageAnnotate1",
      "successors": [
        "ImageOverlay"
      ]
    },
    {
      "module": "aiko_services.elements.image_io",
      "name": "ImageAnnotate2",
      "successors": [
        "ImageOverlay"
      ]
    },
    {
      "module": "aiko_services.elements.image_io",
      "name": "ImageOverlay",
      "successors": [
        "VideoShow"
      ]
    },
    {
      "module": "aiko_services.elements.video_io",
      "name": "VideoShow",
      "parameters": {
        "window_location": [
          50,
          50
        ],
        "window_title": "Astra"
      },
      "successors": [
        "ImageResize"
      ]
    },
    {
      "module": "aiko_services.elements.image_io",
      "name": "ImageResize",
      "parameters": {
        "new_height": 360,
        "new_width": 640
      },
      "successors": [
        "VideoWriteFile"
      ]
    },
    {
      "module": "aiko_services.elements.video_io",
      "name": "VideoWriteFile",
      "parameters": {
        "video_frame_rate": 29.97,
        "video_pathname": "z_output.mp4"
      }
    }
  ]
}